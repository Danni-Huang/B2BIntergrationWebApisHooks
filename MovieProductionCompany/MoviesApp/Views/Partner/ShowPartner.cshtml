@model PartnerViewModel

@{
    ViewData["Title"] = "Streaming partner details";
}

<div>
    <p>
        <a asp-controller="Home" asp-action="Index">Home</a> 
        <span class="mx-1">|</span>
        <a asp-controller="Movie" asp-action="GetAllMovies">movies</a> 
        <span class="mx-1">|</span>
